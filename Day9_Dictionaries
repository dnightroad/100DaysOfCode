#student scoring with dictionaries 
student_scores = {
  "Harry": 81,
  "Ron": 78,
  "Hermione": 99, 
  "Draco": 74,
  "Neville": 62,
}

student_grades = {}
for student in student_scores:
  score = student_scores[student]
  if score < 70:
    student_grades.update({student:"Fail"})
  if score < 80 and score >= 71:
    student_grades.update({student:"Acceptable"})
  if score <90 and score >= 81:
    student_grades.update({student:"Exceeds Expectations"}
    )
  if score <100 and score >= 91:
    student_grades.update({student:"Outstanding"}
    )

print(student_grades)





